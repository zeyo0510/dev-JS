




export method _getADX_(_DMI_ this) =>
    float retValue = 00.00
    ////////////////////////////////////////////////////////////
    float plusDI  = this._getPlusDI_()
    float minusDI = this._getMinusDI_()
    ////////////////////////////////////////////////////////////
    float dx = _Calc_DX_(plusDI, minusDI)
    ////////////////////////////////////////////////////////////
    retValue := _Calc_DX_AVG_(dx, this._adx_smoothing_)
    ////////////////////////////////////////////////////////////
    retValue