




export method _getPositiveDI_(_DMI_ this) =>
    float retValue = 00.00
    ////////////////////////////////////////////////////////////
    float dm = _DM_(_Signed_._Positive_)
    float tr = _TR_(                   )
    ////////////////////////////////////////////////////////////
    float adm = _DM_AVG_(dm, this._di_length_)
    float atr = _TR_AVG_(tr, this._di_length_)
    ////////////////////////////////////////////////////////////
    retValue := _DI_(adm, atr)
    ////////////////////////////////////////////////////////////
    retValue